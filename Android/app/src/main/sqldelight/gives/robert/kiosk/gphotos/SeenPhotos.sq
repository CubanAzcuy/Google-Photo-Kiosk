CREATE TABLE SeenPhotos (
  id TEXT NOT NULL,
  album_id TEXT NOT NULL,
  url TEXT NOT NULL,
  mime_type TEXT NOT NULL
);

selectAll:
SELECT *
FROM SeenPhotos;

selectWithAlbumId:
SELECT *
FROM SeenPhotos
WHERE album_id = :album_id;

insert:
INSERT INTO SeenPhotos(id, album_id, url, mime_type)
VALUES (?, ?, ?, ?);

count:
SELECT count(*) FROM SeenPhotos;


